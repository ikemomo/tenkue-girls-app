<h3>フォロワー</h3>
<ul>
  <% if @followers.count != 0 %>
    <% @followers.each do |follower| %>
      <li><%= link_to follower.name, user_path(follower.id) %></li>
    <% end %>
  <% else %>
    <p>まだフォロワーがいません</p>
  <% end %>
  <%= link_to "フォロー中", user_follow_path(@user.id) %>
  <%= link_to "マイページに戻る", user_path(@user.id)%>
</ul>
