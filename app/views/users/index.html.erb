<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<ol>
  <% @users.each do |user| %>
    <li>
      <%# プロフィール画像 %>
      <%= image_tag user.image.url, width: 50 %>
      <%# ユーザー名 %>
      <%= user.name %>
      <%# 年齢 %>
      <% unless user.birthday.nil? %>
        <%= (Date.today.strftime('%Y%m%d').to_i - user.birthday.strftime('%Y%m%d').to_i) / 10000  %>
      <% end %>
      <%# エンジニア歴 %>
      <% unless user.engineer_start_at.nil? %>
        <%= (Date.today.strftime('%Y%m%d').to_i - user.engineer_start_at.strftime('%Y%m%d').to_i) / 10000  %>
      <% end %>
      <%# 雇用形態 %>
      <%# 婚姻歴 %>
      <%# 育児状況 %>
      <%# 介護状況 %>
      <%# コメント %>
    </li>
  <% end %>
</ol>
